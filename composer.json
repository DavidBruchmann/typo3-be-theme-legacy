{
    "name": "wdb/be-theme-legacy",
    "type": "typo3-cms-extension",
    "description": "Displayes colored Icons in the backend of TYPO3 v14 like in former versions.",
    "license": "GPL-3.0-or-later",
	"homepage": "https://github.com/DavidBruchmann/typo3-be-theme-legacy/",
	"support": {
		"issues": "https://github.com/DavidBruchmann/typo3-be-theme-legacy/issues"
	},
	"keywords": [
		"TYPO3",
		"CMS",
		"backend",
		"theme",
		"extension",
		"icons",
		"modules"
	],
	"authors": [
		{
			"name": "David Bruchmann",
			"email": "david.bruchmann@gmail.com",
			"homepage": "https://github.com/DavidBruchmann/",
			"role": "Developer"
		}
	],
    "require": {
        "typo3/cms-core": "^14.1.0 || dev-main",
        "typo3/cms-backend": "^14.1.0 || dev-main"
    },
    "autoload": {
        "psr-4": {
            "WDB\\BeThemeLegacy\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "be_theme_legacy"
        }
    }
}
